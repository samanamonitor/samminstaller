[
	{
		"object_type": "check",
		"name": "snmp_ns_info",
		"command": "snmp_vars",
		"mib_variable_name_list": [
			"NS-ROOT-MIB::sysBuildVersion",
			"NS-ROOT-MIB::sysIpAddress",
			"NS-ROOT-MIB::sysNetmask",
			"NS-ROOT-MIB::sysGateway",
			"NS-ROOT-MIB::sysHardwareVersionDesc",
