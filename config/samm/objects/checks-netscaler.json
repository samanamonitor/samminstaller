			"NS-ROOT-MIB::sysModelId",
			"NS-ROOT-MIB::haCurState",
			"NS-ROOT-MIB::numPEs",
			"NS-ROOT-MIB::sslCards",
			"NS-ROOT-MIB::haNicsMonitorFailed"
		],
		"tag_properties": [
			"sysBuildVersion",
			"sysIpAddress",
			"sysNetmask",
			"sysGateway",
			"sysHardwareVersionDesc",
			"sysCustomID",
			"sysHighAvailabilityMode",
			"sysHardwareSerialNumber",
			"sysModelId",
			"haCurState",
			"numPEs",
			"sslCards",
			"haNicsMonitorFailed"
		],
		"alias": "snmp_ns_info",
		"check_interval": 600,
		"stale_timeout": 1800,
		"metrics": [
			"sysCurMappedIpCount"
		]

	},
