	{
		"object_type": "check",
		"name": "cim_ctxsf_events",
		"alias": "cim_ctxsf_events",
		"logfile": "Citrix Delivery Services",
		"use": [
			"template_cim_events"
		]
	},
	{
		"object_type": "check",
		"name": "ctx_login",
		"command": "odata_ctx_events",
		"alias": "ctx_login",
		"check_interval": 300,
		"time_property": "StartDate",
		"entity": "Sessions",
		"expand": "User,Machine,CurrentConnection",
		"filter": {
			"Fn::join": [[
				"StartDate gt DateTime'",
				{ "Fn::edmsecondsfromnow": [ 300 ] },
				"' and ExitCode eq 1"
			]]
		},
		"tags": {
			"event_type": "login",
			"job": { "Fn::ref": [ "tags.job" ] },
			"logfile": "ctx_logs"
		},
		"tag_property": {
			"sessionkey": "SessionKey",
			"userupn": "User.Upn",
			"machinednsname": "Machine.DnsName"
		},
		"stale_timeout": 900,
		"metrics": [
			"ls_events"
		]
	},
